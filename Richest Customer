public int MaximumWealth(int[][] accounts) {
     
        int max_wealth = 0;
        for(int i = 0; i < accounts.Length; i++)
        {
            int wealth = 0;
            foreach(int j in accounts[i])
            {
                wealth += j;
            }
            
            max_wealth = Math.Max(wealth, max_wealth);
        }
        
        return max_wealth;
}
